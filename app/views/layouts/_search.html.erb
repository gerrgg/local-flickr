 <%= form_for('', method: :get,
                            enforce_utf8: false,
                            html: { id: 'search', class: 'form-inline' } ) do |f| %>
    <%= f.text_field :s, class: 'form-control', value: params[:s] %>
    <%= f.submit 'Search', class: 'btn btn-dark', name: nil %>
<% end %>